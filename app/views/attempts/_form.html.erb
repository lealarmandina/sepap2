<%= form_for([@problem, @problem.attempts.build]) do |f| %>
  <div class="field">
    Language
    <br />
    <select id="language" name="language">
      <option value="C">C/C++</option>
      <option>Java</option>
    </select>
  </div>
  <div class="field" id="divSubir">
    <fieldset>
      <legend><%= f.label :code, 'Source code' %></legend>
      <br />
      <%= f.file_field :code %>
    </fieldset>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
