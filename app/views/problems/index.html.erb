<h1>Listing problems</h1>

<div id="table_container">
  <table class="data_table">
  <thead>
  <tr>
    <th>Title</th>
    <th>ID</th>
  </tr>
  </thead>
  <tbody>
<% @problems.each do |problem| %>
  <tr onclick="location.href='<%= problem_path(problem) %>'" style='cursor:pointer'>
    <td><%=  problem.title %></td>
    <td><%=  problem.id %></td>
  </tr>
<% end %>
  </tbody>
</table>

<br />
